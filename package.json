{
  "name": "@themost/cli",
  "version": "2.1.1",
  "description": "Most Web Framework command line utility",
  "homepage": "https://github.com/kbarbounakis/most-web-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/kbarbounakis/most-web-cli.git"
  },
  "keywords": [
    "most",
    "web",
    "cli"
  ],
  "author": {
    "name": "Kyriakos Barbounakis",
    "email": "k.barbounakis@gmail.com"
  },
  "licenses": [
    {
      "type": "BSD-3 Clause"
    }
  ],
  "bin": {
    "themost": "bin/themost"
  },
  "scripts": {
    "cli": "node bin/themost",
    "build": "gulp build",
    "watch": "gulp watch"
  },
  "dependencies": {
    "async": "*",
    "ejs": "^2.5.9",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.5",
    "mkdirp": "*",
    "moment": "^2.11.2",
    "pluralize": "^7.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/yargs": "^11.0.0",
    "assert": "^1.4.1",
    "babel-cli": "^6.22.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.0.0",
    "gulp-duration": "0.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-newer": "^1.3.0",
    "gulp-print": "^2.0.1",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "mocha": "^5.0.5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true
    },
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "after": false
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off",
      "no-invalid-this": "warn",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "no-var": [
        "error"
      ],
      "strict": [
        2,
        "never"
      ]
    }
  }
}
