{
  "name": "sample-express-app",
  "version": "0.1.0",
  "description": "Sample Express Application with @themost as ORM",
  "main": "dist/server.js",
  "repository": "",
  "private": true,
  "author": "",
  "scripts": {
    "start": "node bin/www",
    "build": "babel server --out-dir dist/server --copy-files --source-maps --retain-lines",
    "watch": "babel server --out-dir dist/server --watch --copy-files --source-maps --retain-lines",
    "themost": "themost"
  },
  "devDependencies": {
    "babel-cli": "^6.22.0",
    "babel-core": "^6.22.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-preset-env": "^1.1.8"
  },
  "dependencies": {
    "@themost/common": "^2.1.4",
    "@themost/data": "^2.1.3",
    "@themost/sqlite": "^2.1.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "ejs": "^2.5.9",
    "ejs-locals": "^1.0.2",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "source-map-support": "^0.5.5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true
    },
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "after": false
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off",
      "no-invalid-this": "warn",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "no-var": [
        "error"
      ],
      "strict": [
        2,
        "never"
      ]
    }
  }
}
