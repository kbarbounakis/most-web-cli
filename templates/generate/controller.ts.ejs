import {HttpBaseController} from '@themost/web';
import {httpController, httpGet, httpAction} from '@themost/web/decorators';

@httpController()
class <%-name%>Controller extends HttpBaseController {

    constructor() {
        super();
    }

    @httpGet()
    @httpAction('index')
    getIndex() {
        return this.view();
    }

}

export = <%-name%>Controller;
